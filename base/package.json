{
  "name": "simple-boilerplate-v2",
  "author": "Hone Watson",
  "version": "0.3.0",
  "description": "A Boilerplate for ES6 Browser Apps using Babel, Webpack, FlowType, ESLint, Mocha and Chai",
  "main": "build/index.js",
  "keywords": [
    "es6",
    "babel",
    "eslint",
    "npm package"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bin/_mocha",
    "webpack-production": "webpack --progress -p --config webpack.production.config.js",
    "rollup-production": "rollup -c rollup.production.config.js --environment INCLUDE_DEPS,BUILD:production",
    "server": "node ./server.js",
    "clean": "rm -rf build",
    "flow": "flow check",
    "pretest": "npm run flow && npm run build",
    "lint": "eslint src test"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.38.0",
    "isparta": "^4.0.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.2.2",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sass-loader": "^4.1.1",
    "semantic-release": "^6.3.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honewatson/simple-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/honewatson/simple-boilerplate/issues"
  },
  "homepage": "https://github.com/honewatson/simple-boilerplate#readme",
  "license": "MIT"
}
